{
  "name": "aviral/amvirgin",
  "type": "project",
  "description": "AmVirgin Entertainment Media Pvt. Ltd.",
  "keywords": [
	"amvirgin",
	"aviral",
	"laravel"
  ],
  "license": "proprietary",
  "require": {
	"php" : "7.4.*",
	"ext-dom" : "*",
	"ext-json" : "*",
	"ext-libxml" : "*",
	"barryvdh/laravel-dompdf" : "^0.8.6",
	"bensampo/laravel-enum" : "^v3.0.0",
	"binary-cats/laravel-sku" : "^0.3.2",
	"fideloper/proxy" : "^4.4",
	"laravel/framework" : "^v8.0",
	"laravel/socialite" : "^5.1",
	"laravel/tinker" : "^2.5",
	"niklasravnsborg/laravel-pdf" : "^4.0",
	"pbmedia/laravel-ffmpeg" : "^7.4",
	"phpoffice/phpspreadsheet" : "^1.14",
	"razorpay/razorpay" : "^2.5",
	"spatie/laravel-sluggable" : "^2.2",
	"staudenmeir/laravel-adjacency-list" : "^1.0",
	"tymon/jwt-auth" : "1.0.*",
	"yoeunes/notify" : "^1.0",
	"zanysoft/laravel-zip" : "^1.0",
	"zumba/json-serializer" : "^2.2"
  },
  "require-dev": {
	"facade/ignition": "^2.3.6",
	"fzaninotto/faker": "^1.9.1",
	"laravel/ui": "^v3.0",
	"mockery/mockery": "^1.3.1",
	"nunomaduro/collision": "^5.0",
	"phpunit/phpunit": "^9.0"
  },
  "config": {
	"optimize-autoloader": true,
	"preferred-install": "dist",
	"sort-packages": true
  },
  "extra": {
	"laravel": {
	  "dont-discover": []
	}
  },
  "autoload": {
	"psr-4": {
	  "App\\": "app/"
	},
	"classmap": [
	  "database/seeders",
	  "database/factories"
	],
	"files": [
	  "bootstrap/helpers.php"
	]
  },
  "autoload-dev": {
	"psr-4": {
	  "Tests\\": "tests/"
	}
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
	"post-autoload-dump": [
	  "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
	  "@php artisan package:discover --ansi"
	],
	"post-root-package-install": [
	  "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
	],
	"post-create-project-cmd": [
	  "@php artisan key:generate --ansi"
	]
  },
  "authors": [
	{
	  "name": "Aviral Singh",
	  "email": "dev.aviralx@gmail.com",
	  "role": "Lead Backend Developer"
	}
  ],
  "homepage": "http://amvirgin.com"
}
