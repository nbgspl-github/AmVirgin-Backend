{
	"name" : "aviral/amvirgin",
	"type" : "project",
	"description" : "AmVirgin Entertainment Media Pvt. Ltd.",
	"keywords" : [
		"amvirgin",
		"aviral",
		"laravel"
	],
	"license" : "proprietary",
	"require" : {
		"php": "7.4.*",
		"ext-json": "*",
		"barryvdh/laravel-dompdf": "^0.8.6",
		"bensampo/laravel-enum": "^1.38",
		"binary-cats/laravel-sku": "^0.2.0",
		"fideloper/proxy": "^4.0",
		"jeremeamia/superclosure": "^2.4",
		"laravel/framework": "^6.2",
		"laravel/tinker": "^1.0",
		"niklasravnsborg/laravel-pdf": "^4.0",
		"phpoffice/phpspreadsheet": "^1.14",
		"spatie/laravel-sluggable": "^2.2",
		"staudenmeir/laravel-adjacency-list": "^1.0",
		"sudiptpa/guid": "^1.0",
		"tightenco/ziggy": "^0.8.1",
		"tymon/jwt-auth": "1.0.*",
		"yoeunes/notify": "^1.0",
		"zanysoft/laravel-zip": "^1.0",
		"zumba/json-serializer": "^2.2",
      "ext-libxml": "*",
      "ext-dom": "*"
    },
	"require-dev": {
		"facade/ignition": "^1.4",
		"fzaninotto/faker": "^1.4",
		"laravel/ui": "^1.1",
		"mockery/mockery": "^1.0",
		"nunomaduro/collision": "^3.0",
		"phpunit/phpunit": "^8.0"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/"
		},
		"classmap": [
			"database/seeds",
			"database/factories"
		],
		"files": [
			"bootstrap/helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"post-autoload-dump" : [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi"
		],
		"post-root-package-install" : [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd" : [
			"@php artisan key:generate --ansi"
		]
	},
	"authors" : [
		{
			"name" : "Aviral Singh",
			"email" : "dev.aviralx@gmail.com",
			"role" : "Lead Backend Developer"
		}
	],
	"homepage" : "http://amvirgin.com"
}
